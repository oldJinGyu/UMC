<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity_rel">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/arin"
            android:layout_width="200dp"
            android:layout_height="350dp"
            android:src="@drawable/arin4" />

        <ImageView
            android:layout_width="200dp"
            android:layout_height="350dp"
            android:layout_marginLeft="11dp"
            android:layout_toRightOf="@id/arin"
            android:src="@drawable/arin5" />

        <ImageView
            android:id="@+id/arin2"
            android:layout_width="200dp"
            android:layout_height="350dp"
            android:layout_below="@id/arin"
            android:layout_marginTop="32dp"
            android:src="@drawable/arin6" />

        <ImageView
            android:layout_width="200dp"
            android:layout_height="350dp"
            android:layout_marginLeft="11dp"
            android:layout_toRightOf="@id/arin2"
            android:layout_alignBottom="@id/arin2"
            android:src="@drawable/arin7" />

    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>